<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="by.dao.RateResultDao">


    <select id="getRateResults" resultType="by.entity.RateResult">
        select
            id,
            result as rateResult
        from rate_result
    </select>

    <select id="getRateResult" resultType="by.entity.RateResult">
        select
            id,
            result as rateResult
        from rate_result
        where id = #{rateResultId}
    </select>

</mapper>